    "housing",
    "villains",
    "airbnb"
  ],
  "author": "Christie Charles April Zach",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Numbericons/LairBnB/issues"
  },
  "homepage": "https://github.com/Numbericons/LairBnB#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.6.4",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "validator": "^11.1.0"
  },
  "devDependencies": {
    "nodemon": "^1.19.1"
  }
